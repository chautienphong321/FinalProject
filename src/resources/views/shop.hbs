      {{! Shop filter }}
      <section id="shop-filter" class="menu py-2">
        <div class="container">

          <div class="row">
            <div class="col-lg-12 d-flex justify-content-center">
              <ul id="shop-flters">
                <li data-filter="*" class="filter-active">Show All</li>
                <li data-filter="shop-filter-men">Men</li>
                <li data-filter="shop-filter-women">Women</li>
                <li data-filter="shop-filter-unisex">Unisex</li>
              </ul>

            </div>
            <div class="col-lg-12 d-flex justify-content-center">
              <div class="form-range">
                <input
                  type="range"
                  class="form-range"
                  min="0"
                  max="100"
                  step="1"
                  id="price-range"
                />
                <span class="range-value">$2,500</span>
              </div>
            </div>
          </div>

        </div>
      </section>
      {{! End Shop filter }}

